{
  "manifest_version": 2,
  "name": "Recent Tabs",
  "version": "0.0.4",
  "description": "Recent Tabs",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content-script.js"
      ],
      "css": [
        "css/content-css.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "commands": {
    "save": {
      "suggested_key": {
        "default": "Ctrl+E"
      },
      "description": "trigger"
    }
  }
}
